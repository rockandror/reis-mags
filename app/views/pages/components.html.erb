<%= content_tag :div, class:"container" do %>
  <!-- Popover -->
  <div class="row">
    <h2>Popover</h2>
    <div class="col-xs-3">
      <%= link_to 'Popover Top',
                  '#',
                  class: 'btn btn-primary',
                  data: {
                    toggle: 'popover',
                    placement:'top',
                    trigger: 'focus',
                    content: 'Some content inside the popover',
                    turbolinks: false} ,
                  title: "Popover Left" %>
    </div>
    <div class="col-xs-3">
      <%= link_to 'Popover Left',
                  '#',
                  class: 'btn btn-primary',
                  data: {
                    toggle: 'popover',
                    placement:'left',
                    trigger: 'focus',
                    content: 'Some content inside the popover',
                    turbolinks: false},
                  title: "Popover Left" %>
    </div>
    <div class="col-xs-3">
      <%= link_to 'Popover Right',
                  '#',
                  class: 'btn btn-primary',
                  data: {
                    toggle: 'popover',
                    placement:'right',
                    trigger: 'focus',
                    content: 'Some content inside the popover',
                    turbolinks: false},
                  title: "Popover Left" %>
    </div>
    <div class="col-xs-3">
      <%= link_to 'Popover Bottom',
                  '#',
                  class: 'btn btn-primary',
                  data: {
                    toggle: 'popover',
                    placement:'bottom',
                    trigger: 'focus',
                    content: 'Some content inside the popover',
                    turbolinks: false},
                  title: "Popover Left" %>
    </div>

  </div>
  <hr>
  <!-- ToolTips -->
  <div class="row">
    <h2>ToolTips</h2>
    <div class="col-xs-3">
      <%= link_to 'Hover over me', '#', class: 'btn btn-primary', data: {toggle: 'tooltip', placement: 'top'}, title: "Yes Hover top" %>
    </div>
    <div class="col-xs-3">
      <%= link_to 'Hover over me', '#', class: 'btn btn-primary', data: {toggle: 'tooltip', placement: 'left'}, title: "Yes Hover Left" %>
    </div>
    <div class="col-xs-3">
      <%= link_to 'Hover over me', '#', class: 'btn btn-primary', data: {toggle: 'tooltip', placement: 'right'}, title: "Yes Hover Right" %>
    </div>
    <div class="col-xs-3">
      <%= link_to 'Hover over me', '#', class: 'btn btn-primary', data: {toggle: 'tooltip', placement: 'bottom'}, title: "Yes Hover Bottom" %>
    </div>
  </div>
  <hr>
  <!-- Modal -->
  <div class="row">
    <h2>Modal</h2>
    <div class="col-xs-12">
      <%= link_to 'Open Modal', '#myModal', class: 'btn btn-primary', data: {toggle: 'modal', target: '#myModal'} %>
    </div>

    <%= content_tag :div, class: "modal fade", id: 'myModal', role: 'dialog' do %>
      <%= content_tag :div, class: "modal-dialog" do %>
        <%= content_tag :div, class: "modal-content" do %>
          <%= content_tag :div, class: "modal-header" do %>
            <%= link_to '#myModal', class: 'close', data: {dismiss: 'modal'} do %>
              &times;
            <% end %>
            <h4 class="modal-title">Modal Header</h4>
          <% end %>
          <%= content_tag :div, class: "modal-body" do %>
            <p>Some text in the modal.</p>
          <% end %>
          <%= content_tag :div, class: "modal-footer" do %>
            <%= link_to 'Close', '#myModal', class: 'btn btn-primary', data: {dismiss: 'modal'} %>
          <% end %>
        <% end %>
      <% end %>
    <% end %>

  </div>
  <hr>
  <!-- Collapse -->
  <div class="row">
    <h2>Collapse</h2>
    <h4>Simple Collapse</h4>
    <%= link_to 'Simple collapsible', '#demo', class: 'btn btn-primary', data: {toggle:"collapse"} %>
